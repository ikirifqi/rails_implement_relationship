<h1 class="page-title">Import Data</h1>

<%= form_tag importer_import_file_path, :multipart => true do %>
  <div class="form-group">
    <p>Please select the file from your computer to be imported</p>
    <%= file_field_tag :sample %>
  </div>

  <div class="form-group">
    <%= submit_tag 'Import Data', :class => 'btn btn-success' %>
  </div>
<% end %>
